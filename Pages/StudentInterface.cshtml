    <script>

        function onCourseSelect() {
            var courseSelect = document.getElementById("courseSelectMarks");
            var selectedCourseCode = courseSelect.options[courseSelect.selectedIndex].value;
            window.location.href = "/Marks?courseCode=" + selectedCourseCode;
        }
        function changeCardHeader(selectedCourseId) {
            const courseSelect = document.getElementById("courseSelectAttendance");
            const selectedOption = courseSelect.options[courseSelect.selectedIndex];
            const selectedCourseCode = selectedOption.text.split(' - ')[0];
            const selectedCourseName = selectedOption.text.split(' - ')[1];
            const attendanceHeader = document.getElementById("attendanceTitle");
            attendanceHeader.innerText = selectedCourseCode + " - " + selectedCourseName;

            const attendanceTable = document.getElementById("attendanceTable");
            const attendanceRows = attendanceTable.querySelectorAll("tbody tr");

            attendanceRows.forEach(row => {
                const courseCode = row.getAttribute("data-course-code");
                if (courseCode !== selectedCourseCode) {
                    row.style.display = "none";
                } else {
                    row.style.display = "";
                }
            });

            var totalDays = 0;
            var daysAttended = 0;
            var progressBars = document.querySelectorAll("[data-course-code='" + selectedCourseCode + "'] .progress-bar");
            for (var i = 0; i < progressBars.length; i++) {
                var progressBar = progressBars[i];
                totalDays++;
                if (progressBar.getElementById("data-status") === "Present" || progressBar.getAttribute("data-status") === "Late") {
                    daysAttended++;
                }
            }
            var attendancePercentage = (daysAttended * 100) / totalDays;
            var progressBars = document.querySelectorAll("[data-course-code='" + selectedCourseCode + "'] .progress-bar");
            for (var i = 0; i < progressBars.length; i++) {
                var progressBar = progressBars[i];
                progressBar.style.width = attendancePercentage + "%";
                progressBar.setAttribute("aria-valuenow", attendancePercentage);
                progressBar.innerHTML = attendancePercentage + "%";
            }
        }

       
        function changeMarksHeader(selectedCourseId) {
            console.log("ok");
            const courseSelect = document.getElementById("courseSelectMarks");
            const selectedOption = courseSelect.options[courseSelect.selectedIndex];
            const selectedCourseCode = selectedOption.text.split(' - ')[0];
            const selectedCourseName = selectedOption.text.split(' - ')[1];
            const attendanceHeader = document.getElementById("marksCardHeader");
            attendanceHeader.innerText = selectedCourseCode + " - " + selectedCourseName;

            const evaluationTypeSelect = document.getElementById("evaluationTypeSelect");
            const selectedEvaluationType = evaluationTypeSelect.options[evaluationTypeSelect.selectedIndex].value;

            // Call function to update table with evaluation data
            getEvaluationData(selectedCourseCode, selectedEvaluationType);
        }


        function getEvaluationData(selectedCourseCode, selectedEvaluationType) {
            // Get data from Model.enrolledCourses and filter by selected course code and evaluation type
            const courseData = @Json.Serialize(Model.enrolledCourses);
            const selectedCourseData = courseData.filter(c => c.id.startsWith(selectedCourseCode))[0];
            const evaluationData = selectedCourseData.evaluations.filter(e => e.evaluationType == selectedEvaluationType)[0];

            // Update table rows with evaluation data
            const table = document.getElementById("marksTable");
            const rows = table.getElementsByTagName("tr");
            const headerRow = rows[0];
            const dataRow = rows[1];

            // Update header row
            headerRow.cells[1].innerText = `Weightage (${evaluationData.evalWeightage}%)`;
            headerRow.cells[2].innerText = `Obtained Marks (${evaluationData.obtained_marks}/${evaluationData.total_marks})`;
            headerRow.cells[3].innerText = `Total Marks (${evaluationData.total_marks})`;
            headerRow.cells[4].innerText = `Average (${evaluationData.average})`;
            headerRow.cells[5].innerText = `Minimum (${evaluationData.min})`;
            headerRow.cells[6].innerText = `Maximum (${evaluationData.max})`;

            // Update data row
            dataRow.cells[1].innerText = `${evaluationData.evalWeightage}%`;
            dataRow.cells[2].innerText = `${evaluationData.obtained_marks}/${evaluationData.total_marks}`;
            dataRow.cells[3].innerText = `${evaluationData.total_marks}`;
            dataRow.cells[4].innerText = `${evaluationData.average}`;
            dataRow.cells[5].innerText = `${evaluationData.min}`;
            dataRow.cells[6].innerText = `${evaluationData.max}`;
        }





