        <div id="Attendance" class="tabcontent">
            <div class="position-absolute" style="top: 20%; left: 42%;">
                <div style="max-width: 600px; margin: 0 auto; padding: 20px;">
                    <form class="form-example" action="" method="post">
                        <div class="form-group">
                            <label for="courseSelectAttendance">Select a course:</label>
                        <select class="form-control" id="courseSelectAttendance">
                            @foreach (var course in Model.enrolledCourses)
                            {
                                <option>@course.id - @course.name</option>
                            }

                            </select>
                        </div>
                    </form>
                </div>
            </div>
        <div style="position: relative;">
            <div class="progress" style="position: absolute; top: 110px; left: 117%; width: 30%">
                <div class="progress-bar bg-success" role="progressbar" style="width: 80%;" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100">80%</div>
            </div>
        </div>

            <div class="card" style="top: 130px; left: 28%; width: 120%">
                <div class="card-header" id="attendanceTitle">CSCI101 - Introduction to Computer Science</div>
                <div class="card-body">
                    <table class="table"  id="attendanceTable">
                        <thead>

                            <tr>
                                <th>Date</th>
                                <th>Duration (In Hours)</th>
                                <th>Presence</th>
                            </tr>
                        </thead>


                        <tbody>




                        @foreach (var attendance in Model.courseAttendances)
                        {

                            <tr data-course-code="@attendance.courseCode">
                                <td>@attendance.Date</td>
                                    <td>@attendance.contactHours</td>
                                    <td id="status">@attendance.Status</td>
                                </tr>
                            
                        }

                     
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

        <div id="Transcript" class="tabcontent">
        <div class="position-absolute" style="top: 20%; left: 48%;">
            <div style="max-width: 600px; margin: 0 auto; padding: 20px;">
                <form class="form-example" action="" method="post">
                    <div class="form-group">
                        <label for="semSelect">Select the semester:</label>
                        <select class="form-control" id="semSelect" style="text-align:center">
                            @{
                                var uniqueSemesters = Model.transcripts.Select(t => t.Semester).Distinct();
                            }
                            @foreach (var semester in uniqueSemesters)
                            {
                                <option>@semester</option>
                            }
                        </select>
                    </div>
                </form>
            </div>
        </div>

        @{
            var totalGradePoints = Model.transcripts.Sum(t => t.GetGradePoints() * t.CreditHours);
            var totalCreditHours = Model.transcripts.Sum(t => t.CreditHours);
            var cgpa = totalCreditHours > 0 ? totalGradePoints / totalCreditHours : 0;
        }
        <p style="font-weight:bold; font-style:italic; position:absolute; left: 80%; top: 37%">CGPA: @String.Format("{0:F2}", cgpa)</p>

            <div class="card" style="top: 130px; left: 28%; width: 120%">
            @foreach (var semesterGroup in Model.transcripts.GroupBy(t => t.Semester))
            {
                var semester = semesterGroup.Key;

                <div class="card-header">@semester</div>
                <div class="card-body">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>Code</th>
                                <th>Course Name</th>
                                <th>Section</th>
                                <th>CrdHrs</th>
                                <th>Grade</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var transcript in semesterGroup)
                            {
                                <tr>
                                    <td>@transcript.CourseID</td>
                                    <td>@transcript.Name</td>
                                    <td>@transcript.Section</td>
                                    <td>@transcript.CreditHours</td>
                                    <td>@transcript.Grade</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            }
        </div>
    </div>
