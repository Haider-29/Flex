@page
@using System.Text.Json;

@model FLEXX.Pages.StudentInterfaceModel



    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.6.0/css/bootstrap.min.css" />

        <title>Student Portal</title>
        <style>
            * {
                box-sizing: border-box
            }

            body {
                /*font-family: "Lato", sans-serif;*/
                background-color: #f8f9fa;
            }

            /* Style the tab */
            .tab {
                margin-top: 12%;
                /*margin-right: 20px;*/
                float: left;
                border: none;
                background-color: #2A363B;
                width: 12%;
                height: 70%;
                position: fixed;
                top: 0;
                border-radius: 20px;
                left: 10%;
            }


                /* Style the buttons inside the tab */
                .tab button {
                    border-radius: 20px;
                    margin-top: 0%;
                    display: block;
                    background-color: inherit;
                    color: white;
                    padding: 22px 13px;
                    width: 100%;
                    border: none;
                    outline: none;
                    text-align: center;
                    cursor: pointer;
                    transition: 0.3s;
                    font-size: 17px;
                }

                    /* Change background color of buttons on hover */
                    .tab button:hover {
                        background-color: #282A3A;
                    }

                    /* Create an active/current "tab button" class */
                    .tab button.active {
                        background-color: #282A3A;
                        color: #F7C402;
                    }

            /* Style the tab content */
            .tabcontent {
                margin-top: 20px;
                float: left;
                padding: 0px 20px;
                border: none;
                width: 70%;
                border-left: none;
                height: 95%;
            }

            .container {
                margin-left: 200px;
                text-align: center;
            }

            .form-title {
                font-size: 24px;
                font-weight: bold;
                margin-top: 20px;
                margin-bottom: 20px;
            }

            .table-title {
                font-size: 24px;
                font-weight: bold;
                margin-top: 50px;
                margin-bottom: 20px;
            }

            .card {
                margin-top: 30px;
                text-align:left;
            }

            .card-header {
                background-color: #3f51b5;
                color: white;
                font-weight: bold;
            }

            .btn-primary {
                background-color: #3f51b5;
                border-color: #3f51b5;
                font-weight: bold;
                margin-top: 20px;
                margin-bottom: 20px;
            }

            .form-group {
                margin-bottom: 20px;
            }

            .form-line {
                margin-bottom: 5%;
            }

            .form-label {
            margin-top: 10px;
            font-style: italic;
            }
            .progress {
                width: 40%; 
                top: 30%; 
                left: 55%;
            }
        </style>
    </head>
    <body>

        <h1 style="margin-left: 10%"><b>Student Portal</b></h1>

        <div class="tab">
            <button class="tablinks" onclick="menu(event, 'Home')" id="defaultOpen">Home</button>
            <button class="tablinks" onclick="menu(event, 'Marks')">Marks</button>
            <button class="tablinks" onclick="menu(event, 'Attendance')">Attendance</button>
            <button class="tablinks" onclick="menu(event, 'Transcript')">Transcript</button>
            <button class="tablinks" onclick="menu(event, 'Course Registration')">Course Registration</button>
            <button class="tablinks" onclick="menu(event, 'Feedback')">Feedback</button>
        </div>

        <div id="Home" class="tabcontent">
            <div class="container">
                <div class="card">
                    <div class="card-header">University Information</div>
                    <div class="card-body">
                        <p>
                            <b>Roll No.: </b> @Model.StudentId
                            <br>
                            <b>Semester: </b> @Model.currStudent.Semester
                        </p>
                    </div>
                </div>

                <div class="card">
                    <div class="card-header">Student Information</div>
                    <div class="card-body">
                        <p>
                            
                            <b>Name: </b> @Model.currStudent.Name
                            <br />
                            <b>Section: </b> @Model.currStudent.section
                            
                        </p>
                    </div>
                </div>

                <div class="card">
                    <div class="card-header">Semeter GPA</div>
                    <div class="card-body">
                        <div style="width: 100%">
                            <canvas id="myChart"></canvas>
                        </div>
                    </div>
                </div>
            </div>
        </div>

