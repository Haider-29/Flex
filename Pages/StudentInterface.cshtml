        <style>
            * {
                box-sizing: border-box
            }

            body {
                /*font-family: "Lato", sans-serif;*/
                background-color: #f8f9fa;
            }

            /* Style the tab */
            .tab {
                margin-top: 12%;
                /*margin-right: 20px;*/
                float: left;
                border: none;
                background-color: #2A363B;
                width: 12%;
                height: 70%;
                position: fixed;
                top: 0;
                border-radius: 20px;
                left: 10%;
            }


                /* Style the buttons inside the tab */
                .tab button {
                    border-radius: 20px;
                    margin-top: 0%;
                    display: block;
                    background-color: inherit;
                    color: white;
                    padding: 22px 13px;
                    width: 100%;
                    border: none;
                    outline: none;
                    text-align: center;
                    cursor: pointer;
                    transition: 0.3s;
                    font-size: 17px;
                }

                    /* Change background color of buttons on hover */
                    .tab button:hover {
                        background-color: #282A3A;
                    }

                    /* Create an active/current "tab button" class */
                    .tab button.active {
                        background-color: #282A3A;
                        color: #F7C402;
                    }

            /* Style the tab content */
            .tabcontent {
                margin-top: 20px;
                float: left;
                padding: 0px 20px;
                border: none;
                width: 70%;
                border-left: none;
                height: 95%;
            }

            .container {
                margin-left: 200px;
                text-align: center;
            }

            .form-title {
                font-size: 24px;
                font-weight: bold;
                margin-top: 20px;
                margin-bottom: 20px;
            }

            .table-title {
                font-size: 24px;
                font-weight: bold;
                margin-top: 50px;
                margin-bottom: 20px;
            }

            .card {
                margin-top: 30px;
                text-align:left;
            }

            .card-header {
                background-color: #3f51b5;
                color: white;
                font-weight: bold;
            }

            .btn-primary {
                background-color: #3f51b5;
                border-color: #3f51b5;
                font-weight: bold;
                margin-top: 20px;
                margin-bottom: 20px;
            }

            .form-group {
                margin-bottom: 20px;
            }

            .form-line {
                margin-bottom: 5%;
            }

            .form-label {
            margin-top: 10px;
            font-style: italic;
            }
            .progress {
                width: 40%; 
                top: 30%; 
                left: 55%;
            }
        </style>
    </head>
