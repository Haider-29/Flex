        <div id="Course Registration" class="tabcontent">
            <div class="container">
                <!-- Available Courses Table -->
                <div class="card">
                    <div class="card-header">Offered Courses</div>
                    <div class="card-body">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>Course ID</th>
                                    <th>Course Name</th>
                                    <th>Credit Hours</th>
                                    <th>Semester</th>
                                </tr>
                            </thead>
                            <tbody>


                            @foreach(var offerCourse in Model.offeredCourses)
                            {
                                <tr>
                                    <td>@offerCourse.CourseID</td>
                                    <td>@offerCourse.CourseTitle</td>
                                    <td>@offerCourse.CreditHours</td>
                                    <td>@offerCourse.Semester</td>
                                </tr>
                            }
                    
                               
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="card">
                    <div class="card-header">Register for Offered Course</div>
                    <div class="card-body">
                        <form method="post" asp-page-handler="RegisterForCourse">
                            <div class="form-group">
                                <label for="courseSelect">Select a course:</label>
                                <select asp-for="RegisterCourseID" class="form-control" id="courseSelectRegister">
                                @foreach(var currCourse in Model.offeredCourses)
                                {


                                    <option>
                                      

                                        
                                        @currCourse.CourseID - @currCourse.CourseTitle</option>
                                }
                                </select>
                            </div>
                        <p class="alert alert-danger mt-3 text-center" role="alert" style="@(!string.IsNullOrEmpty(Model.registerMessage) ? "" : "display:none;")">@Model.registerMessage</p>

                            <button type="submit" class="btn btn-primary">Submit</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>

        <div id="Feedback" class="tabcontent">
